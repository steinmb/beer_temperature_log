{
    "name": "steinmb/onewire",
    "description": "Communicate with 1 wire devices",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Stein Magne Bjorklund",
            "email": "steinmb@smbjorklund.com"
        }
    ],
    "require": {
        "php": ">=8.2",
        "ext-bcmath": "*",
        "ext-sockets": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.2"
        }
    },
    "autoload": {
        "psr-4": {
            "steinmb\\": "src/"
        }
    },
    "require-dev": {
        "phan/phan": "4.x",
        "phpstan/phpstan": "^1.9",
        "phpunit/phpunit": "^10"
    }
}
